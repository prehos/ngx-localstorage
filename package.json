{
  "name": "ngx-localstorage",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx run ngx-localstorage-demo:serve:development",
    "lint": "nx run ngx-localstorage:lint",
    "test": "nx run ngx-localstorage:test",
    "build:lib": "nx run ngx-localstorage:build:production && npm run copy-assets",
    "copy-assets": "copyfiles ./LICENSE ./README.md ./dist/libs/ngx-localstorage",
    "docs": "npm run docs:generate && npm run docs:build",
    "docs:generate": "nx run ngx-localstorage:doc-gen && cp ./dist/docs/ngx-localstorage.html ./apps/ngx-localstorage-demo/src/app/components/api/api.component.html",
    "docs:build": "nx run ngx-localstorage-demo:build:production --output-path docs --base-href /ngx-localstorage/ --skip-nx-cache"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@nrwl/angular": "^15.7.2",
    "marked": "^4.2.4",
    "ngx-markdown": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular-devkit/core": "^15.2.0",
    "@angular-devkit/schematics": "^15.2.0",
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/template-parser": "^15.0.0",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/language-service": "^15.2.0",
    "@nrwl/cli": "^15.7.2",
    "@nrwl/cypress": "^15.7.2",
    "@nrwl/eslint-plugin-nx": "^15.7.2",
    "@nrwl/jest": "^15.7.2",
    "@nrwl/linter": "^15.7.2",
    "@nrwl/workspace": "^15.7.2",
    "@schematics/angular": "~15.1.0",
    "@types/jest": "28.1.8",
    "@types/marked": "^4.0.8",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "all-contributors-cli": "^6.20.0",
    "autoprefixer": "^10.4.0",
    "copyfiles": "^2.4.1",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-deprecation": "^1.3.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "ng-packagr": "15.1.2",
    "ngx-doc-gen": "^2.1.1",
    "nx": "15.7.2",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
